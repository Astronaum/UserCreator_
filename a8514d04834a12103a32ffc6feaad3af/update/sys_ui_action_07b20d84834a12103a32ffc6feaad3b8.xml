        <script><![CDATA[function createSCIMUser() {
    var username = g_form.getValue('username');
    var displayname = g_form.getValue('displayname');
    var email = g_form.getValue('email');
	alert(username);
	alert(displayname);
	alert(email);

    if (!username || !displayname || !email) {
        alert("Please fill in all fields.");
        return;
    }

    // Call Script Include via GlideAjax
    var ga = new GlideAjax('CreateSCIMUser');
	alert("Im here");
    ga.addParam('sys_id', g_form.getUniqueValue());
    ga.addParam('username', username);
    ga.addParam('displayname', displayname);
    ga.addParam('email', email);
    ga.getXMLAnswer(function(response) {
        var result = response.responseXML.documentElement.getAttribute("answer");
        alert(result);
    });
}]]></script>
